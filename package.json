{
  "name": "node-test-app",
  "version": "0.3.0",
  "description": "A sample Node.js app using Express 4",
  "engines": {
    "node": "10.x"
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf build && mkdir build",
    "build": "npm run clean && babel src -d dist",
    "server": "nodemon dist/index.js --exec babel-node",
    "heroku-postbuild": "npm install",
    "start": "babel-node dist/index.js"
  },
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-cli": "^6.26.0",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "path": "^0.12.7",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "nodemon": "^1.18.10",
    "request": "^2.81.0",
    "tape": "^4.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/heroku/node-test-app"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT",
  "heroku-run-build-script": true
}
